{
  "name": "snapp-credit-check",
  "description": "",
  "version": "1.0.0",
  "author": "Mina Protocol <contact@minaprotocol.com>",
  "url": "https://github.com/MinaProtocol/mina/issues",
  "homepage": "https://github.com/MinaProtocol/mina#readme",
  "license": "Apache-2.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "nodemon --exec electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dist": "electron-builder"
  },
  "dependencies": {
    "puppeteer": "^7.1.0"
  },
  "devDependencies": {
    "electron": "^11.3.0",
    "electron-builder": "^22.10.5",
    "nodemon": "^2.0.7"
  },
  "build": {
    "appId": "org.minaprotocol.snapp-credit-demo",
    "extraResources": [
      "node_modules",
      {
        "from": "resources/bin",
        "to": "Resources/bin",
        "filter": [
          "**/*"
        ]
      }
    ],
    "files": [
      "!node_modules"
    ],
    "dmg": {
      "contents": [
        {
          "x": 160,
          "y": 220
        },
        {
          "x": 370,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ],
      "sign": false
    },
    "linux": {
      "target": [
        "deb"
      ]
    },
    "deb": {
      "depends": [
        "libjemalloc2",
        "libffi7",
        "libssl1.1",
        "libgmp10",
        "libgomp1",
        "apt-transport-https",
        "curl",
        "ca-certificates",
        "libnss3",
        "libatk1.0-0",
        "libatk-bridge2.0-0",
        "libgdk-pixbuf2.0-0",
        "libgtk-3-0",
        "libgbm-dev"
      ]
    },
    "win": {
      "target": "NSIS"
    }
  }
}
