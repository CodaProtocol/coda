  "main": "index.js",
  "dependencies": {
    "bs-css": "^8.0.2",
    "bs-electron": "github:Schmavery/bs-electron",
    "graphql_ppx": "^0.2.8",
    "reason-apollo": "^0.15.2",
    "reason-react": "github:reasonml/reason-react#6a854ec"
  },
  "devDependencies": {
