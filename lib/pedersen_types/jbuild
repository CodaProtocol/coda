(jbuild_version 1)

(library
 ((name pedersen_types)
  (public_name pedersen_types)
  (flags (:standard -short-paths -warn-error -9-27-58))
  (library_flags (-linkall))
  (inline_tests)
  (libraries (core async async_extra))
  (preprocess (pps (ppx_jane)))
  (synopsis "Pedersen types are all the types needed for windowing to work")))

