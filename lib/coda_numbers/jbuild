(jbuild_version 1)

(library
 ((name coda_numbers)
  (public_name coda_numbers)
  (flags (:standard -short-paths))
  (inline_tests)
  (library_flags (-linkall))
  (libraries (snark_bits snark_params unsigned_extended protocols core snarky crypto_params))
  (preprocess (pps (ppx_jane ppx_deriving.eq)))
  (synopsis "Snark-friendly numbers used in Coda consensus")))
