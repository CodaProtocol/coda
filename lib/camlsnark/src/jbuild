(library
 ((name camlsnark)
  (public_name camlsnark)
  (libraries (core_kernel camlsnark_c))
  (c_library_flags (:standard -lstdc++ -lpthread))
  (flags (:standard -safe-string))
  (cxx_flags
    (
      (:standard "\\" -pedantic)
      (-I re2_c/libre2)
      ))
  (preprocess (pps (ppx_jane ppx_driver.runner)))))

(jbuild_version 1)

