(jbuild_version 1)

(library
 ((name spawner)
  (public_name spawner)
  (library_flags (-linkall))
  (libraries (core async rpc_parallel linear_pipe parallel file_system))
  (preprocess (pps (ppx_jane ppx_deriving.eq)))
  (flags (:standard -short-paths -warn-error -27-33-9))
  (synopsis "Spawner lets you run multiple workers on different nodes")))
