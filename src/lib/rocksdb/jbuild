(jbuild_version 1)

(library
 ((name rocksdb)
  (public_name rocksdb)
  (flags (:standard -short-paths -warn-error -58))
  (library_flags (-linkall))
  (c_library_flags (-lrocksdb))
  (libraries (core rocks key_value_database))
  (preprocess (pps (ppx_jane)))
  (synopsis "RocksDB Database module")))
